#include "core/version.h"

namespace senscord {

std::string CoreVersion::Name() { return "SensCord"; }

uint32_t CoreVersion::Major() { return @SensCord_VERSION_MAJOR@; }

uint32_t CoreVersion::Minor() { return @SensCord_VERSION_MINOR@; }

uint32_t CoreVersion::Patch() { return @SensCord_VERSION_PATCH@; }

std::string CoreVersion::Description() {
  return "@SensCord_VERSION_DESCRIPTION@";
}

}  // namespace senscord
